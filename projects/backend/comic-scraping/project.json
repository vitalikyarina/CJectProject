{
  "name": "backend-comic-scraping",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "projects/backend/comic-scraping/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/projects/backend/comic-scraping",
        "main": "projects/backend/comic-scraping/src/main.ts",
        "tsConfig": "projects/backend/comic-scraping/tsconfig.app.json",
        "assets": [
          "projects/backend/comic-scraping/src/assets"
        ],
        "isolatedConfig": true,
        "webpackConfig": "projects/backend/comic-scraping/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "backend-comic-scraping:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "backend-comic-scraping:build:development"
        },
        "production": {
          "buildTarget": "backend-comic-scraping:build:production"
        }
      }
    }
  },
  "tags": []
}
