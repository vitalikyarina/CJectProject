{
  "name": "backend-comic",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "backend/comic/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "backend/comic/dist",
        "main": "backend/comic/src/main.ts",
        "tsConfig": "backend/comic/tsconfig.app.json",
        "assets": [
          "backend/comic/src/assets"
        ],
        "isolatedConfig": true,
        "webpackConfig": "backend/comic/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "backend-comic:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "backend-comic:build:development"
        },
        "production": {
          "buildTarget": "backend-comic:build:production"
        }
      }
    }
  },
  "tags": []
}
